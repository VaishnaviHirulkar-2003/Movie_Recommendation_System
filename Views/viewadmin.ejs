<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title>Admin Table</title>
  <style>
    .admin {
      padding: 20px;
      font-family: Arial, sans-serif;
    }

    table {
      width: 100%;
      border-collapse: collapse;
    }

    th, td {
      padding: 10px;
      text-align: left;
      border: 1px solid #ddd;
    }

    thead {
      background-color: #f2f2f2;
    }

    tr:hover {
      background-color: #f9f9f9;
    }
  </style>
</head>
<body>
  <div class="admin">
    <table>
      <thead>
        <tr>
          <th>Sr</th>
          <th>Username</th>
          <th>Email</th>
          <th>Created At</th>
          <th>Updated At</th>
        </tr>
      </thead>
      <tbody>
           <% if (admin.length > 0) { %>
          <% user.forEach((u, index) => { %>
            <tr>
              <td><%= index + 1 %></td>
              <td><%= u.adminname %></td>
              <td><%= u.email %></td>
              <td><%= u.created_time %></td>
              <td><%= u.updated_time %></td>
            </tr>
          <% }); %>
        <% } else { %>
          <tr>
            <td colspan="5"><strong>No Data Available</strong></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
</body>
</html>
